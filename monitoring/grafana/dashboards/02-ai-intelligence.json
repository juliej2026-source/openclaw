  "fiscalYearStartMonth": 0,
  "liveNow": false,
  "templating": {
    "list": [
      {
        "name": "station",
        "type": "query",
        "query": "label_values(hivemind_station_reachable, station)",
        "datasource": {
          "type": "prometheus",
          "uid": "ds-prometheus"
        },
        "includeAll": true,
        "multi": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "refresh": 2,
        "sort": 1
      },
      {
        "name": "model",
        "type": "query",
        "query": "label_values(hivemind_perf_success_rate, model_id)",
