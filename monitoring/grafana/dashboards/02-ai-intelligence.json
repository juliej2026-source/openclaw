{
  "id": null,
  "uid": "openclaw-02",
  "title": "AI Intelligence & Model Performance",
  "description": "OpenClaw Hive Mind AI model fleet status, task distribution, performance matrix, and traces",
  "tags": ["openclaw", "hive-mind", "ai", "models", "performance"],
  "timezone": "browser",
