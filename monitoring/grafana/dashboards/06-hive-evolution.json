    {
      "id": 5,
      "title": "Model Success Rate by Task Type",
      "description": "KEY PANEL: Tracks model performance evolution over time. Each line represents a model+task_type combination showing success rate percentage.",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "hivemind_perf_success_rate{model_id=~\"$model\",task_type=~\"$task_type\"}*100",
          "legendFormat": "{{model_id}} / {{task_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 5,
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": 3600000,
            "gradientMode": "none",
            "axisBorderShow": false
          },
          "unit": "percent",
          "decimals": 1,
          "min": 0,
          "max": 100,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["lastNotNull", "mean", "max", "min"],
          "sortBy": "Last *",
          "sortDesc": true
        }
      }
    },
